type Query {
  allBooks: [Book]
  findOne(id: ID!): Book
  findOneApartment(id: ID!): Apartment
  allApartments: [Apartment]
}

type Apartment {
  id: ID!
  webAddress: String
  address: String
  price: String
  numOfRooms: String
  squareMeters: String
}


type Book {
  id: ID!
  title: String
  pages: Int
  rating: Rating
  author: Author
}

type Rating {
  rating: String
  star: String
}

type Author {
  id: ID!
  firstName: String
  lastName: String
  books: [Book]
}